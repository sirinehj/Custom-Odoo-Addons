<odoo>
    <!--menu view-->
    <record id="model_b_many2one_menu_view" model="ir.actions.act_window">
        <field name="name">Testing_01 Model B many2one</field>
        <field name="res_model">model.b.many2one</field>
        <field name="view_mode">list,form</field>
        <field name="type">ir.actions.act_window</field>
    </record>
    <!--tree view-->
    <record id="model_b_many2one_list_view" model="ir.ui.view">
        <field name="name">Testing_01 Model B many2one tree view</field>
        <field name="model">model.b.many2one</field>
        <field name="arch" type="xml">
            <list string="Model B many2one tree list">
                  <field name="numb"/>
                  <field name="model_a_id"/>
            </list>
        </field>
    </record>
    <!--form view-->
    <record id="model_b_many2one_form_view" model="ir.ui.view">
        <field name="name">Testing_01 Model B many2one form view</field>
        <field name="model">model.b.many2one</field>
        <field name="arch" type="xml">
            <form string="Model B many2one form">
                <sheet>
                    <group>
                        <field name="numb"/>
                        <field name="model_a_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!--search view-->
    <record id="model_b_many2one_search_view" model="ir.ui.view">
        <field name="name">Testing_01 Model B many2one search view</field>
        <field name="model">model.b.many2one</field>
        <field name="arch" type="xml">
            <search string="Model B many2one tree list">
                  <field name="numb"/>
                  <field name="model_a_id"/>
                  <filter name="group_by_numb_of_model_b_many2one" context="{'group_by':'numb'}"/>
                  <filter name="group_by_model_a_many2one" context="{'group_by':'model_a_id'}"/>
            </search>
        </field>
    </record>
</odoo>